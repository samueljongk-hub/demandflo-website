{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x",
      "includeFiles": "api/lib/**"
    },
    "api/**/*.js": {
      "runtime": "nodejs20.x",
      "includeFiles": "api/lib/**"
    }
  }
}


